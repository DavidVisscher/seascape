Current page: <%= inspect(@state.ephemeral.current_page) %>

<ul>
  <%= for cluster <- @state.persistent.clusters do %>
     <li phx-click="ephemeral/change_page" phx-value-page="clusters/show/<%= cluster.id %>" >
       <%= cluster.name %>
     </li>
     <% end %>
</ul>

<button phx-click="persistent/cluster/create">Add Cluster</button>
